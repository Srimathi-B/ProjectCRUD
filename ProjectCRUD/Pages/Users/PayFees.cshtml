@page
@model ProjectCRUD.Pages.Users.PayFeesModel
@{
}

<form method="post" class="shadow-lg p-3 mb-5 bg-white rounded">
    <div class="form-group">
        <label asp-for="@Model.Plan_Start"></label>
        <input asp-for="@Model.Plan_Start" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="@Model.Plan_End"></label>
        <input asp-for="@Model.Plan_End" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="@Model.Amount"></label>
        <input asp-for="@Model.Amount" class="form-control" />
    </div>
    <div class="form-group">
        <h6 class="text-success">@Model.SuccessMessage</h6>
        <h6 class="text-danger">@Model.ErrorMessage</h6>
        <h6 class="text-danger">@Html.ValidationSummary()</h6>
    </div>
    <div class="row gx-0">
        <a href="/Users/Download" class="col btn btn-default border-secondary ms-3" role="button">Register</a>
        <a href="/Users/ListBook" class="col btn btn-default border-secondary ms-3" role="button">Cancel</a>
    </div>
</form>
